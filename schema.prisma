datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model GoogleToken {
  id  String  @id
  access_token  String?
  refresh_token  String?
  scope  String?
  token_type  String?
  expiry_date  Int?
}

enum Role {
  USER
  ADMIN
}